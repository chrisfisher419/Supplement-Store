@model SupplementStore.WebUI.Models.ProductsListViewModel
@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="keywords" content="">
    <title>Supplement Store</title>
    <!-- Bootstrap -->
    <link href="~/nutrition/css/bootstrap.min.css" rel="stylesheet">
    <!-- Style CSS -->
    <link href="~/nutrition/css/style.css" rel="stylesheet">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=PT+Serif:400,400i,700,700i" rel="stylesheet">
    <link href="~/nutrition/css/owl.carousel.min.css" rel="stylesheet">
    <link href="~/nutrition/css/owl.theme.default.css" rel="stylesheet">
    <!-- FontAwesome CSS -->
    <link rel="stylesheet" type="text/css" href="~/nutrition/css/fontello.css">
    <link href="~/nutrition/css/font-awesome.min.css" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <div class="header-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                    <a href="index.html"><img src="./images/logo.png" alt=""></a>
                </div>
                <div class="col-lg-8 col-md-10 col-sm-12 col-xs-12">
                    <div class="navigation">
                        <div id="navigation">
                            <ul>
                                @Html.Action("Summary", "Cart")
                                <li>

                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="space-medium bg-light">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="section-title">
                        <!-- section title start-->
                        <div class="col-xs-12 col-sm-8">
                            
                            @foreach (var p in Model.Products)
                            {
                                @Html.Partial("ProductSummary", p)
                            }
                            <div class="pager">
                                @Html.PageLinks(Model.PagingInfo, x => Url.Action("List",
                                    new { page = x, category = Model.CurrentCategory }))
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.section title start-->
            </div>
        </div>
        <div>

    </div>
    </div>

    </div>
    <!-- testimonials-close  -->
    <!-- footer start -->
    <div class="footer">
        <div class="container">
            <div class="row">
                <!-- footer-about-start -->
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 ">
                    <div class="footer-widget">
                        <h3 class="footer-title">Contact Infomation</h3>
                        <div class="">
                            <ul>
                                <li><i class="icon-envelope"></i>christopher.m.clute@outlook.com</li>
                                <li> <i class="icon-envelope"></i>christopher.fisher419@outlook.com</li>

                                @*<li><i class="icon-envelope"></i>info@dietcoach.com</li>*@
                            </ul>
                        </div>
                       
                </div>
        
                <!-- footer-tiny-text-start -->
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="tiny-footer">
                        <p>Template from <a href="https://easetemplate.com/downloads/jessica-nutritionist-dietician-website-template/">Easetemplate</a></p>
                        <p></p>
                    </div>
                </div>
                <!-- footer-tiny-text-start -->
                <!-- footer-form-close -->
            </div>
        </div>
    </div>

    @*@RenderSection("scripts", required: false)*@
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/nutrition/js/jquery.min.js" type="text/javascript"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/nutrition/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/nutrition/js/navigation.js" type="text/javascript"></script>
    <script src="~/nutrition/js/menumaker.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/nutrition/js/jquery.sticky.js"></script>
    <script type="text/javascript" src="~/nutrition/js/sticky-header.js"></script>
    <script type="text/javascript" src="~/nutrition/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/nutrition/js/slider.js"></script>
    <script type="text/javascript" src="~/nutrition/js/testimonial-slider.js"></script>
</body>

</html>